{
  "name": "cli-cypress-parallel",
  "version": "1.0.0",
  "description": "run cypress tests in multiple parallel processes",
  "type": "commonjs",
  "main": "cypress_parallel/cypress-parallel.js",
  "scripts": {
    "cy:parallel": "npx cli-cypress-parallel",
    "cy:parallel:test": "npm run cy:parallel -- -t 6 -d './test/cypress/integration/**' --configFile './test/cypress.json' -o './test/reporter-config.json'"
  },
  "author": "willrichboa",
  "license": "MIT",
  "dependencies": {
    "cli-table3": "^0.6.3",
    "colors": "^1.4.0",
    "cypress": "9.7.0",
    "cypress-multi-reporters": "^1.6.3",
    "glob": "^10.0.0",
    "yargs": "^17.7.1"
  },
  "bin": {
    "cli-cypress-parallel": "cypress_parallel/cypress-parallel.js"
  }
}